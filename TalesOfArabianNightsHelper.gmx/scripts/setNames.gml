game = oGameData.id

for (i = 0; i < game.numOfPlayers; i+=1 ){
    if game.colorPicked[i] = 0 {game.playerName[i]="Ma'aruf"}
    if game.colorPicked[i] = 1 {game.playerName[i]="Ali Baba"}
    if game.colorPicked[i] = 2 {game.playerName[i]="Scheherazade"}
    if game.colorPicked[i] = 3 {game.playerName[i]="Zumurrud"}
    if game.colorPicked[i] = 4 {game.playerName[i]="Aladdin"}
    if game.colorPicked[i] = 5 {game.playerName[i]="Sinbad"}
}
